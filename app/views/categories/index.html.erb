<h1 align="center">Listing All Categories</h1>
<div align="center">
  <%= will_paginate %>
  <% @categories.each do |category| %>
    <div class="row">
      <div class="col-md-4 offset-md-4 card text-center bg-light">
        <div class= "card-body">
          <%= link_to "#{category.name}", category_path(category) %>
        </div>
      </div>
    </div>
  <% end %>
  <%= will_paginate %>
</div>